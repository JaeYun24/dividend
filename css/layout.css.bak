@charset "utf-8";

/*-------------------------------------------------------------------
    파일정의 : 레이아웃 및 컨텐츠 (PC)
-------------------------------------------------------------------*/

/*[s] Skip Navigation */
	.skipnav{width: 100%;}
	.skipnav a{position: absolute;left: -3000px;}
	.skipnav a:focus{display: block;top: 0;left: 0;z-index: 1000;width: 100%;height: 30px;background: #222447;font-size: 15px;line-height: 30px;color: #fff;text-align: center;border:none;}
/*[e] Skip Navigation */

/*[s] All Style */
	html{width: 100%;overflow-x:hidden;overflow-y:auto;}
	html,body{width: 100%;font-size:22px;}
	html.no-scroll{overflow-y:hidden;}

	/* 공용 */
	.infit{font-size:0;letter-spacing:0;word-spacing:0;}
	.arl{font-family:"Arial" !important;}
	.mob-ver{display:none;}
	.smob-ver{display:none;}
	.pc-ver{display:block;}
/*[e] All Style */

/*[s] Layout Style */
	#wrapper{position:relative;width:100%;min-width:320px;}
	#header{position: absolute;top:0;left:0;width: 100%;height:80px;z-index:999;transition: all 0.3s;}
	#header h1{padding:30px 0 0 80px;transition: all 0.2s;}
	#header h1 *{display:inline-block;vertical-align: top;}
	#header h1 a span.mark{display:inline-block;width:131px;height:20px;background:url('../images/logo.png') no-repeat;background-size:auto 20px;font-size:0;}
	#header h1 a span.tt{position:relative;margin-top:-1px;margin-left:10px;font-size:22px;font-weight:600;color:#fff;}
	#header .gnb-wrap{position:absolute;top:30px;left:50%;transform: translateX(-50%);z-index:2;}
	#header .gnb-depth1>li{display: inline-block;margin:0 32px;}
	#header .gnb-depth1>li>a{position:relative;display: block;padding-bottom:33px;font-size: 18px;font-weight:600;color: #fff}
	#header .gnb-depth1>li>a:before{position: absolute;bottom: 0px;left: 50%;width: 0;height: 4px;background: none;transform: translateX(-50%);transition:  all 0.4s;content: ''}
	#header .gnb-depth1>li.on>a{position: relative;color: #fff}
	#header .gnb-depth1>li.on>a::before{width: 100%;background: #fff;}
	#header .gnb-depth2{display: none;position: absolute;top: 18px;left:50%;transform: translateX(-50%);width:100%;min-width:400px;padding-top:30px;text-align:center;box-sizing: border-box;}
	#header .gnb-depth2>li{display:inline-block;vertical-align: top;padding:0 30px;}
	#header .gnb-depth2>li>a{display: block;font-weight: 600;font-size: 18px;color: #fff;line-height:80px;word-break: keep-all}
	#header .tnb{position:absolute;top:20px;right:80px;transition: all 0.2s;}
	#header .tnb > ul > li{display: inline-block;position: relative;}
	#header .tnb > ul > li a{display: block;font-size: 18px;font-weight:600;line-height:40px;color: #fff;}
	#header .tnb > ul > li.rbtn a{position:relative;padding-right:40px;background:url('../images/ico-link.png') no-repeat right 1px center;}
	#header .tnb > ul > li.rbtn a::before{position:absolute;left:-24px;top:-7px;z-index:-1;display:inline-block;width:140px;height:56px;background:none;border-radius:0;content:'';opacity:0;transition: all 0.3s;}
	#header .tnb > ul > li.rbtn a:hover::before{background:#000E65;border-radius:30px;opacity:1;}
	#header .tnb > ul > li a.join{margin-right:44px;}
	#header .tnb > ul > li .info{display:none;margin-right:24px;padding:0 16px;background:#666;border-radius:5px;}
	#header .tnb > ul > li .info span{font-size:18px;font-weight:500;line-height:40px;color:#fff;}
	#header .tnb > ul > li .info span:first-child::after{display:inline-block;vertical-align: top;height:18px;margin:12px 8px;border-right:1px solid #999;content:'';}
	#header .tnb > ul > li .info.stk-mem{background:#1428A0;}
	#header .tnb > ul > li .info.stk-mem span:first-child::after{border-right:1px solid #fff;opacity:0.5;}
	#header .tnb.is-login > ul > li.rbtn a::before{display:none;}
	#header .tnb.is-login > ul > li > a.join{display:none;}
	#header .tnb.is-login > ul > li .info{display:inline-block;}
	#header .mgnb-open{display:none;}
	#header .dim{display:none;}
	/* active */
	#header.active::before{position:absolute;top:80px;left:0;z-index:2;display:block;width:100%;border-bottom:1px solid #fff;opacity:0.2;content:'';}

	/* sub header */
	#header.sub{position:relative;background:#fff;}
	#header.sub h1 a span.mark{display:inline-block;width:131px;height:20px;background:url('../images/logo-blue.svg') no-repeat;background-size:auto 20px;font-size:0;}
	#header.sub h1 a span.tt{color:#000E65;}
	#header.sub .gnb-depth1>li>a{color:#000;}
	#header.sub .gnb-depth1>li.on>a::before{background: #000;}
	#header.sub .gnb-depth2>li>a{color:#000;}
	#header.sub .tnb > ul > li a{color:#000;}
	#header.sub .tnb > ul > li.rbtn a{background:url('../images/ico-link-black.png') no-repeat right 1px center;}
	#header.sub .tnb > ul > li.rbtn a:hover{background:url('../images/ico-link.png') no-repeat right 1px center;}
	#header.sub .tnb > ul > li.rbtn a:hover{color:#fff;}
	#header.sub .dim{position:absolute;top:80px;left:0;width:100%;height:80px;background:#fff;}
	/* active */
	#header.sub.active::before{border-bottom:1px solid #e1e1e1;opacity:1;}

	#footer{width:100%;padding:42px 0;border-top:1px solid #000;transition: all 0.3s;}
	#footer h2{display:inline-block;position:absolute;width:105px;height:16px;background:url('../images/logo-blue.png') no-repeat;background-size:auto 16px;font-size:0;}
	#footer .foot-content{display:block;width:100%;padding-left:174px;box-sizing: border-box;transition: all 0.3s;}
	#footer .foot-content .foot-menu li{display:inline-block;margin-right:30px;}
	#footer .foot-content .foot-menu li a{font-size:14px;font-weight:400;color:#000;}
	#footer .foot-content .foot-info{padding-top:24px;font-size:14px;font-weight:400;color:#000;}
	#footer .foot-content .foot-info span{display:inline-block;margin:0 2px;padding-bottom:5px;color:#999;}
	#footer .foot-content .foot-info a{display:inline-block;margin-left:26px;border-bottom:1px solid #000;font-weight:700;}
	#footer .foot-content copy{font-size:12px;font-weight:400;color:#000;}
	#top-btn{position:absolute;right:0;top:0;padding-right:26px;background:url('../images/ico-top.png') no-repeat right center;background-size:auto 13px;font-size:14px;font-weight:600;color:#000;}

	#container{position:relative;}
	.inner{position:relative;width:100%;max-width:1400px;margin:0 auto;box-sizing:border-box;}
	.inner-s{position:relative;width:100%;max-width:1046px;margin:0 auto;}
	.inner-m{position:relative;width:100%;max-width:692px;margin:0 auto;}
	.inner-s::after, .inner-m::after{display:block;clear:both;content:'';}
/*[e] Layout Style */

/* [s] Main */
	/* 공통 */
	a.more{position:relative;z-index:2;}
	a.more::after{transition: all 0.2s;}
	a.more:hover::after{margin-left:10px !important;}
	a.more span{position:relative;color:#000;}
	a.more span::after{display:block;position:absolute;bottom:2px;width:0;border-bottom:1px solid transparent;content:'';transition: all 0.3s;}
	a.more:hover span::after{width:100%;border-bottom:1px solid #000;}
	a.more.wht span{color:#fff;}
	a.more.wht:hover span::after{border-bottom:1px solid #fff;}

	.main-content .main-intro{position:relative;width:100%;height:870px;}
	.main-content .main-intro::before{display:block;position:absolute;left:0;bottom:0;width:100%;height:320px;background:rgba(0,0,0,0.4);content:'';}
	.main-content .main-intro::after{display:block;clear:both;content:'';}
	.main-content .main-intro .main-visual{position:absolute;top:0;left:0;z-index:-1;width:100%;height:870px;background:url('../images/visual-main.jpg') no-repeat center top;background-size:cover;transition: all 0.3s;}
	.main-content .main-intro .main-title{width:100%;padding:240px 0 140px;font-size:56px;font-weight:600;line-height:1.2;color:#fff;letter-spacing:-0.3px;text-align:center;word-break:keep-all; word-wrap:break-word;transition: all 0.3s;}
	.main-content .main-intro .main-info::after{display:block;clear:both;content:'';}
	.main-content .main-intro .main-info > div{position:relative;width:49.5%;height:auto;min-height:220px;padding:44px 60px 44px 44px;box-sizing:border-box;overflow:hidden;transition: all 0.3s;}
	.main-content .main-intro .main-info > div::after{display:block;clear:both;content:'';}
	.main-content .main-intro .main-info > div .lcon{float:left;}
	.main-content .main-intro .main-info > div .lcon h3{font-size:36px;font-weight:600;color:#000;letter-spacing:-0.3px;}
	.main-content .main-intro .main-info > div .lcon .st{display:block;margin-top:5px;font-size:18px;line-height:1.4;color:#999;}
	.main-content .main-intro .main-info > div .lcon .st.date{padding-left:4px;}
	.main-content .main-intro .main-info > div .lcon a.more{display:block;margin-top:30px;font-size:20px;font-weight:600;color:#000;}
	.main-content .main-intro .main-info > div .lcon a.more::after{display:inline-block;width:10px;height:16px;margin-left:14px;background:url('../images/ico-more.png') no-repeat;background-size:auto 16px;content:'';}
	.main-content .main-intro .main-info > div .rcon{float:right;text-align:right;}
	.main-content .main-intro .main-info > div .rcon .prc{font-size:36px;font-weight:700;color:#DF2C2C;}
	.main-content .main-intro .main-info > div .rcon .prc::before{display:inline-block;vertical-align: top;margin-right:5px;content:'▲';}
	.main-content .main-intro .main-info > div .rcon .prc.up::before{content:'▲';}
	.main-content .main-intro .main-info > div .rcon .prc.down{color:#000E65;}
	.main-content .main-intro .main-info > div .rcon .prc.down::before{content:'▼';}
	.main-content .main-intro .main-info > div .rcon .prc strong{font-size:36px;display:inline-block;}
	.main-content .main-intro .main-info > div .rcon .prc strong.arl{font-weight:600;}
	.main-content .main-intro .main-info > div .rcon .prc span{display:inline-block;}
	.main-content .main-intro .main-info > div .rcon dl{display:inline-block;position:relative;width:160px;margin-top:30px;}
	.main-content .main-intro .main-info > div .rcon dt{position:absolute;font-size:18px;font-weight:500;line-height:26px;color:#999;}
	.main-content .main-intro .main-info > div .rcon dd{font-size:18px;font-weight:400;line-height:26px;color:#000;}
	.main-content .main-intro .main-info > div.stk-info{float:left;background:#fff;}
	.main-content .main-intro .main-info > div.dvd-info{float:right;background:#000E65;}
	.main-content .main-intro .main-info > div.dvd-info .lcon h3{padding-top:20px;color:#fff;}
	.main-content .main-intro .main-info > div.dvd-info .lcon .st{margin-top:5px;color:#ccc;}
	.main-content .main-intro .main-info > div.dvd-info .lcon .st b{color:#fff;}
	.main-content .main-intro .main-info > div.dvd-info .rcon a.login{display: block;padding-right:40px;background:url('../images/ico-link.png') no-repeat right 1px center;font-size: 20px;font-weight:600;line-height:110px;color: #fff;}
	.main-content .main-intro .main-info > div.dvd-info.is-login .lcon h3{padding:0;}
	.main-content .main-intro .main-info > div.dvd-info.is-login .lcon .st{color:#fff;}
	.main-content .main-intro .main-info > div.dvd-info.is-login .lcon a.more{color:#fff;}
	.main-content .main-intro .main-info > div.dvd-info.is-login .lcon a.more::after{background:url('../images/ico-more-wht.png') no-repeat;background-size:auto 16px;}
	.main-content .main-intro .main-info > div.dvd-info.is-login .rcon .prc{color:#fff;}
	.main-content .main-intro .main-info > div.dvd-info.is-login .rcon .prc::before{display:none;}
	.main-content .main-intro .main-info > div.dvd-info.is-login .rcon dt{color:#fff;}
	.main-content .main-intro .main-info > div.dvd-info.is-login .rcon dd{color:#fff;}
	.main-content .main-intro .main-ex h3{display:inline-block;vertical-align: middle;width:44%;max-width:590px;font-size:36px;font-weight:400;line-height:215px;color:#fff;letter-spacing:-0.5px;}
	.main-content .main-intro .main-ex .ex{display:inline-block;vertical-align: middle;width:56%;font-size:18px;font-weight:500;line-height:30px;color:#ddd;}
	.main-content .main-notice{position:relative;margin:80px 0;transition: all 0.3s;}
	.main-content .main-notice .tit{position:absolute;top:0;left:0;width:342px;}
	.main-content .main-notice .tit h3{padding-bottom:10px;font-size:36px;font-weight:600;color:#000;letter-spacing:-0.3px;}
	.main-content .main-notice .tit .st{font-size:20px;font-weight:500;line-height:30px;color:#999;}
	.main-content .main-notice .tit a.more{display:block;margin-top:22px;font-size:20px;font-weight:600;color:#000;}
	.main-content .main-notice .tit a.more::after{display:inline-block;width:10px;height:16px;margin-left:14px;background:url('../images/ico-more.png') no-repeat;background-size:auto 16px;content:'';}
	.main-content .main-notice .ntc-list{width:100%;padding-left:470px;box-sizing: border-box;transition: all 0.2s;}
	.main-content .main-notice .ntc-list ul{border-top:2px solid #000;}
	.main-content .main-notice .ntc-list ul > li{display:block;width:100%;height:94px;border-bottom:1px solid #E1E1E1;}
	.main-content .main-notice .ntc-list ul > li::after{display:block;clear:both;content:'';}
	.main-content .main-notice .ntc-list ul > li > a{display:block;float:left;width:80%;font-size:20px;font-weight:500;color:#000;line-height:94px;text-overflow: ellipsis;white-space: nowrap;overflow: hidden; }
	.main-content .main-notice .ntc-list ul > li .date{float:right;font-family:"Arial", sans-serif;font-size:16px;color:#999;line-height:94px;}
	.main-content .main-notice .ntc-list ul > li.none{padding-top:60px;border:none;text-align:center;}
/* [e] Main */

/* [s] Sub */
	/* 공통 */
	.sub-content{min-height:500px;}
	.sub-title{margin:0 80px;padding:72px 15px 0 15px;text-align:center;}
	.sub-title h2{padding-bottom:42px;font-size:48px;font-weight:600;color:#fff;}
	.sub-title .page-root{padding-bottom:20px;font-size:16px;font-weight:300;color:#fff;opacity:1;text-align:right;}
	.sub-title .page-root span{margin-left:6px;}
	.sub-title .page-root span.bar{font-size:14px;}
	.sub-title .page-root span:last-child{font-weight:700;}
	.sec-cont{position:relative;width:100%;max-width:1400px;min-height:320px;margin:80px auto 100px auto;}
	.sec-cont::after{display:block;clear:both;content:'';}

	/* 소개 */
	.intro-sec{position:relative;margin:0 80px 60px 80px;background:url('../images/bg-intro.jpg') no-repeat center top;background-size:cover;box-sizing: border-box;transition: all 0.3s;}
	.intro-sec .sub-title h2{color:#fff;}
	.intro-sec .sub-title .inner{border-bottom:1px solid rgba(255,255,255,0.2);}
	.intro-sec .sub-title .page-root{color:#fff;opacity:0.5;}
	.intro-sec .intro-tit{padding:108px 0 106px 0;font-size:40px;font-weight:400;color:#fff;line-height:56px;text-align:center;}
	.intro-sec .intro-list{width:100%;padding:70px 0 60px 0;background:rgba(0,0,0,0.5);text-align:center;}
	.intro-sec .intro-list li{display:inline-block;vertical-align: top;width:33.33%;}
	.intro-sec .intro-list li .tt{padding-bottom:30px;font-size:32px;font-weight:600;color:#fff;}
	.intro-sec .intro-list li .tt span{font-weight:400;}
	.intro-sec .intro-list li .st{font-size:18px;font-weight:400;line-height:30px;color:#fff;}

	@media only screen and (max-width:1650px){
		.intro-sec .sub-title .inner{padding:0 20px;}
	}

	/* 주식 정보 */
	.stock-sec .sub-title{background:url('../images/bg-stock.png') no-repeat center top;background-size:cover;}
	.stock-sec .sec-cont{background:#FAFAFA;}
	.stock-sec .sec-cont .ready{font-size:36px;line-height:720px;color:#000;text-align:center;}

	/* 마이페이지 */
	.dvd-sec .sub-title{background:url('../images/bg-dvd.png') no-repeat center top;background-size:cover;}
	.dvd-sec .sec-cont h3{float:left;width:15%;max-width:290px;font-size:36px;font-weight:700;line-height:1.3;}
	.dvd-sec .sec-cont .dvd-content{position:relative;width:100%;}
	.dvd-sec .sec-cont .dvd-content::after{display:block;clear:both;content:'';}
	.dvd-sec .sec-cont .dvd-content .dvd-top{position:absolute;width:100%;z-index:5;}
	.dvd-sec .sec-cont .dvd-content .dvd-top .inner{width:100%;max-width:100%;}
	.dvd-sec .sec-cont .dvd-content .dvd-top h3{position:relative;float:left;width:15%;max-width:290px;margin-top:-2px;font-size:36px;font-weight:700;line-height:1.3;}
	.dvd-sec .sec-cont .dvd-content .dvd-top .select-top{float:right;width:80%;max-width:1045px;}
	.dvd-sec .sec-cont .dvd-content .dvd-top .select-top::after{display:block;clear:both;content:'';}
	.dvd-sec .sec-cont .dvd-content .dvd-top .select-top .year-sel{position:relative;float:left;width:244px;z-index:2;}
	.dvd-sec .sec-cont .dvd-content .dvd-top .select-top .year-sel *{vertical-align: top;}
	.dvd-sec .sec-cont .dvd-content .dvd-top .select-top .year-sel > a{display:inline-block;width:16px;height:28px;margin-top:6px;font-size:0;}
	.dvd-sec .sec-cont .dvd-content .dvd-top .select-top .year-sel > a.prev{background:url('../images/ico-left.png') no-repeat;background-size:auto 28px;}
	.dvd-sec .sec-cont .dvd-content .dvd-top .select-top .year-sel > a.next{background:url('../images/ico-right.png') no-repeat;background-size:auto 28px;}
	.dvd-sec .sec-cont .dvd-content .dvd-top .select-top .year-sel .year{position:relative;display:inline-block;width:180px;margin:0 15px;font-size:36px;color:#000;text-align:center;}
	.dvd-sec .sec-cont .dvd-content .dvd-top .select-top .year-sel .year .select-box{display:none;position:absolute;left:50%;top:-20px;width:160px;height:248px;margin-left:-80px;padding:8px 0 25px 0;background:#fff;border:1px solid #aaa;border-radius:5px;font-size:28px;color:#000;text-align:center;overflow-y:auto;-ms-overflow-style: none;}
	.dvd-sec .sec-cont .dvd-content .dvd-top .select-top .year-sel .year .select-box::-webkit-scrollbar{ display:none;}
	.dvd-sec .sec-cont .dvd-content .dvd-top .select-top .year-sel .year .select-box a{display:block;margin-top:18px;font-family:"Arial", sans-serif;font-size:28px;text-align:center;}
	.dvd-sec .sec-cont .dvd-content .dvd-top .select-top .dvd-tab{position:relative;top:-12px;float:right;width:40%;max-width:256px;padding:8px;background:#eceef8;border-radius:5px;box-sizing:border-box;}
	.dvd-sec .sec-cont .dvd-content .dvd-top .select-top .dvd-tab a{display:inline-block;vertical-align: middle;width:50%;font-size:18px;font-weight:600;line-height:46px;color:#000;text-align:center;}
	.dvd-sec .sec-cont .dvd-content .dvd-top .select-top .dvd-tab a.on{background:#000E65;border-radius:5px;color:#fff;}

	.dvd-sec .sec-cont .dvd-content .dvd-top.fixed{position:fixed;top:0;left:0;width:100%;padding:32px 0 10px 0;background:#fff;border-bottom:1px solid #E1E1E1;}
	.dvd-sec .sec-cont .dvd-content .dvd-top.fixed .inner{max-width:1400px;}
	.dvd-sec .sec-cont .dvd-content .dvd-top.fixed h3{width:20%;font-size:32px;}
	.dvd-sec .sec-cont .dvd-content .dvd-top.fixed h3 br{display:none;}
	.dvd-sec .sec-cont .dvd-content .dvd-top.fixed .select-top .year-sel > a{margin-top:3px;}
	.dvd-sec .sec-cont .dvd-content .dvd-top.fixed .select-top .year-sel .year{width:140px;margin:0 15px;font-size:32px;color:#000;}
	.dvd-sec .sec-cont .dvd-content .dvd-top.fixed .select-top .year-sel .year .select-box{width:120px;height:200px;margin-left:-60px;}
	.dvd-sec .sec-cont .dvd-content .dvd-top.fixed .select-top .year-sel .year .select-box a{margin-top:15px;font-size:22px;}
	.dvd-sec .sec-cont .dvd-content .dvd-top.fixed .select-top .dvd-tab a{font-size:18px;}
	.dvd-sec .sec-cont .dvd-content .dvd-cont{float:right;width:80%;max-width:1045px;padding-top:80px;box-sizing: border-box;}
	.dvd-sec .sec-cont .dvd-content .dvd-cont .cont{position:absolute;top:-99999px;width:100%;max-width:1045px;margin-bottom:70px;}
	.dvd-sec .sec-cont .dvd-content .dvd-cont .cont.show{position:relative;top:0;}
	.dvd-sec .sec-cont .dvd-content .dvd-cont .cont:last-child.show{margin-bottom:0;}
	.dt-table{position:relative;width:100%;margin-top:60px;}
	.dt-table:first-child{margin-top:0;}
	.dt-table h4{position:absolute;top:42px;left:0;font-size:32px;font-weight:600;color:#000;}
	.dt-table .r-table{width:100%;padding-left:120px;box-sizing: border-box;}
	.dt-table .r-table > span{position:relative;display:block;font-size:16px;font-weight:500;color:#000;text-align:right;}
	.dt-table .r-table > table{margin-top:10px;border-top:1px solid #E1E1E1;border-bottom:1px solid #000;text-align:center;}
	.dt-table .r-table > table th{height:50px;background:#FAFAFA;font-size:18px;font-weight:600;}
