	.select-basic{width:100%;height: 48px;padding:0 24px;border:1px solid #aaa;border-radius:5px;background: url('../images/ico-sel.png') no-repeat center right 21px;background-size:auto 10px;font-size: 18px;color:#000;line-height: 48px;box-sizing: border-box;}
	.select-basic:disabled{background:#FAFAFA url('../images/ico-sel.png') no-repeat center right 21px;background-size:auto 10px;color:#888;}
	.select-basic.sm{width:100%;height: 48px;padding:0 20px;border:1px solid #aaa;border-radius:5px;background:#fff url('../images/ico-sel.png') no-repeat center right 21px;background-size:auto 10px;font-size: 18px;color:#000;line-height: 48px;box-sizing: border-box;}

	/* 체크박스 */
	.chk{overflow: hidden;position: absolute;width: 1px;height: 1px;margin: -1px;padding: 0;clip:rect(0,0,0,0);border: 0;appearance:none;-webkit-appearance:none;-moz-appearance:none;-o-appearance}
	.chk+.chk-label{display: inline-block;position: relative;padding-left: 46px;font-size: 24px;line-height: 30px;color: #444;cursor: pointer}
	.chk+.chk-label:after{position: absolute;top:0;left: 0;width: 30px;height: 30px;margin-top:1px;background: url('../images/ico-scheck.png') no-repeat center center;background-size:30px;content: ''}
	.chk:checked+.chk-label{color:#1428A0;}
	.chk:checked+.chk-label:after{background: url('../images/ico-schecked.png') no-repeat center center;background-size:36px;}
	.chk-label:hover span{display:inline-block;padding-bottom:1px;border-bottom:1px solid #1428A0;color:#1428A0;}
	.chk.sm+.chk-label.sm{padding-left: 32px;font-size: 18px;line-height: 24px;}
	.chk.sm+.chk-label.sm:after{width: 24px;height: 24px;margin-top:0px;background-size:24px;}
	.chk.sm:checked+.chk-label.sm:after{background-size:24px;}

	.rchk{overflow: hidden;position: absolute;width: 1px;height: 1px;margin: -1px;padding: 0;clip:rect(0,0,0,0);border: 0;appearance:none;-webkit-appearance:none;-moz-appearance:none;-o-appearance}
	.rchk+.rchk-label{display: inline-block;position: relative;padding-left: 34px;font-size: 22px;line-height: 24px;color: #000;cursor: pointer}
	.rchk+.rchk-label:after{position: absolute;top:0;left: 0;width: 24px;height: 24px;margin-top:1px;background: url('../images/ico-rcheck.png') no-repeat center center;background-size:24px;content: ''}
	.rchk:checked+.rchk-label{color:#1428A0;}
	.rchk:checked+.rchk-label:after{background: url('../images/ico-rchecked.png') no-repeat center center;background-size:24px;}

	/* 버튼 */
	.btn-area{display:block;clear:both;text-align:center;}
	.btn-area::after{display:block;clear:both;content:'';}
	.btn-area .btn-norm{display:inline-block;width:60%;max-width:338px;padding-bottom:4px;background:#000;border-radius:5px;font-size:24px;font-weight:600;color:#fff;line-height:68px;text-align:center;transition: all 0.3s;}
