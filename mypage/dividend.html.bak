<script>
$(window).load(function () {
	//로딩
    $('.loading-wp').fadeOut(500);
});

$(document).ready(function () {
	checkOffset();

	//선택탭
	$(".select-box>a").click(function(){
		var rYear = $(this).text();
		$(".sel-btn span").html(rYear)
	});

	$(".ac-btn").click(function(){
		$(this).parents('tr').toggleClass('on').nextUntil('.tb-main').toggleClass('open');
		//$("tr.tb-sub").addClass('open');
	});

	//분기별 차트
	var barChartData = {
	  labels: [
	    "1분기",
	    "2분기",
	    "3분기",
	    "4분기"
	  ],
	  datasets: [
	    {
	      label: "보통주",
	      backgroundColor: "#98DFE5",
	      borderWidth: 0,
	      data: [1000, 500, 800, 1500]
	    },
	    {
	      label: "우선주",
	      backgroundColor: "#3D95DF",
	      borderWidth: 0,
	      data: [1500, 700, 500, 2000]
	    }
	  ]
	};

	var chartOptions = {
	  responsive: true,
	  maintainAspectRatio: false,
	  tooltips: {
	        enabled: true
	  },
	  legend: {
		display: true,
	    position: "top",
		align: "end",
		labels: {
			usePointStyle: true,
            boxWidth:10,
			fontFamily:'SamsungOneKorean',
			fontSize:18,
			fontStyle:500,
			fontColor: 'black'
        }

	  },
	  title: {
	    display: false
	  },
	  scales: {
	    yAxes: [{
		  gridLines: {
			drawBorder: false
		  },
	      ticks: {
			mirror: true,
	        beginAtZero: true,
			stepSize : 500,
			fontSize : 18,
			padding : 10,
			z:2,
			labelOffset : -15,
			fontColor: 'black'
			//suggestedMax: 2500
	      }
		}],
		xAxes: [{
			gridLines: {
                display:false
            },
			ticks: {
				fontFamily:'SamsungOneKorean',
	  			fontSize : 18,
				fontStyle : 600,
				padding : 10,
				fontColor: 'black'
				//labelOffset: 20
			},
			offsetGridLines: true
        }]
	  }
	}

	window.onload = function() {
	  Chart.defaults.global.datasets.bar.categoryPercentage = 0.4;
	  Chart.defaults.global.datasets.bar.barPercentage = 0.8;
	  var ctx = document.getElementById("canvas").getContext("2d");
	  window.myBar = new Chart(ctx, {
	    type: "bar",
	    data: barChartData,
	    options: chartOptions,
		plugins: [{
          beforeInit:  function(chart, options) {
			  	Chart.Legend.prototype.afterFit = function() {
					if(window.innerWidth > 1025){
						this.height = this.height + 80;
					    chart.height += 80;
				  	}else{
						this.height = this.height + 60;
					    chart.height += 60;
				  	}
				};
			}
        }]
	  });
	};




});
</script>
